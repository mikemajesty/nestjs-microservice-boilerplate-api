{
  "name": "nestjs-boilerplate-microservice-api",
  "version": "1.1.4",
  "description": "NestJs boilerplate api",
  "author": "mike.rodrigues.lima@gmail.com",
  "private": true,
  "license": "MIT",
  "@see": "https://github.com/mikemajesty/nestjs-microservice-boilerplate-api/blob/master/guides/setup/package.md",
  "overrides": {
    "@opentelemetry/api": "^1.8.0"
  },
  "scripts": {
    "prepare": "husky",
    "build": "nest build",
    "start": "npm run migration:run && pm2 start ecosystem.config.js",
    "start:dev": "npm run migration:run && tsc --noEmit && nest start --watch -b swc",
    "start:debug": "npm run migration:run && tsc --noEmit && nest start --debug --watch",
    "prettier": "prettier --write \"src/**/*.json\"",
    "lint": "eslint \"{src,test}/**/*.{ts,js}\" --fix",
    "test": "TS_NODE_PROJECT=tsconfig.json node -r ts-node/register node_modules/.bin/jest --config=test/jest.config.ts --verbose --detectOpenHandles --forceExit",
    "test:cov": "jest --config=\"./test/jest-coverage.config.ts\" --coverage --detectOpenHandles --forceExit && npm run make-badges",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "setup": "docker-compose -f docker-compose-infra.yml down -v --remove-orphans && docker-compose -f docker-compose-infra.yml up -d",
    "docker:build": "docker build -t nestjs-api .",
    "docker:run": "docker run -p 5000:5000 --env-file .env --network host --name nestjs-api --rm nestjs-api",
    "docker:stop": "docker stop nestjs-api 2>/dev/null || true",
    "docker:start": "npm run docker:stop && npm run docker:build && npm run docker:run",
    "scaffold": "./node_modules/@mikemajesty/microservice-crud/bin/microservice-crud",
    "typeorm": "ts-node -r tsconfig-paths/register \"./node_modules/typeorm/cli.js\"",
    "migration-postgres:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create ./src/infra/database/postgres/migrations/Name",
    "migration-postgres:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run --dataSource ./src/infra/database/postgres/config.ts",
    "migration-postgres:undo": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert --dataSource ./src/infra/database/postgres/config.ts",
    "migration-mongo:create": "ts-node -r tsconfig-paths/register ./src/infra/database/mongo/config.ts new -n rename",
    "migration-mongo:run": "ts-node -r tsconfig-paths/register ./src/infra/database/mongo/config.ts up",
    "migration-mongo:undo": "ts-node -r tsconfig-paths/register ./src/infra/database/mongo/config.ts down",
    "migration:run": "concurrently --kill-others-on-fail \"npm run migration-postgres:run\" \"npm run migration-mongo:run\"",
    "test:load": "npx artillery run .artillery/config.yaml --record --key <token> ",
    "check-newest:deps": "npx npm-check-updates",
    "make-badges": "istanbul-badges-readme"
  },
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
    "@mikemajesty/zod-mock-schema": "1.0.18",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/typeorm": "^11.0.0",
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.50.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.50.0",
    "@opentelemetry/instrumentation-http": "^0.208.0",
    "@opentelemetry/instrumentation-mongodb": "^0.61.0",
    "@opentelemetry/instrumentation-pg": "^0.61.0",
    "@opentelemetry/instrumentation-redis-4": "^0.49.0",
    "@opentelemetry/sdk-node": "^0.50.0",
    "axios": "^1.13.2",
    "axios-better-stacktrace": "^2.1.7",
    "axios-retry": "^4.5.0",
    "body-parser": "^2.2.0",
    "class-validator": "^0.14.2",
    "colorette": "^2.0.20",
    "compression": "^1.8.1",
    "convert-pino-request-to-curl": "^1.0.11",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "i18next": "^25.6.3",
    "js-yaml": "^4.1.1",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.7.2",
    "mongo-migrate-ts": "^2.0.0",
    "mongoose": "^8.20.2",
    "mongoose-paginate-v2": "^1.9.1",
    "nestjs-convert-to-curl": "^2.0.6",
    "nestjs-i18n": "^10.5.1",
    "nestjs-otel": "^7.0.1",
    "node-cache": "^5.1.2",
    "nodemailer": "^7.0.10",
    "opossum": "^9.0.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "ping": "^1.0.0",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "pino-loki": "^2.6.0",
    "pino-pretty": "^13.1.2",
    "pm2": "^6.0.13",
    "randexp": "^0.5.3",
    "redis": "^5.10.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1",
    "systeminformation": "^5.27.11",
    "ts-jest": "^29.4.5",
    "typeorm": "^0.3.27",
    "typeorm-naming-strategies": "^4.1.0",
    "typescript": "5.9.3",
    "uuid": "^11.0.0",
    "v8": "^0.1.0",
    "validator-brasil": "^1.0.0",
    "write-excel-file": "^2.3.10",
    "yaml": "^2.8.1",
    "zod": "^4.1.12",
    "zod-i18n-map": "^2.27.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@mikemajesty/microservice-crud": "7.1.2",
    "@nestjs/cli": "^11.0.12",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.9",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/npm": "^13.1.2",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.3",
    "@swc/jest": "^0.2.39",
    "@testcontainers/mongodb": "^11.8.1",
    "@testcontainers/postgresql": "^11.8.1",
    "@testcontainers/redis": "^11.8.1",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.10.1",
    "@types/nodemailer": "^7.0.4",
    "@types/opossum": "^8.1.9",
    "@types/ping": "^0.4.4",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@typespec/compiler": "^1.6.0",
    "artillery": "^2.0.27",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.2.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.14.0",
    "husky": "^9.1.7",
    "istanbul-badges-readme": "^1.9.0",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "npm-check-updates": "^19.1.2",
    "prettier": "^3.6.2",
    "semantic-release": "^25.0.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0"
  }
}
